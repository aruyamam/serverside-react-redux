{
   "parser": "@typescript-eslint/parser",
   "extends": ["airbnb", "plugin:@typescript-eslint/recommended"],
   "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true
      }
   },
   "rules": {
      "react/jsx-filename-extension": [
         1,
         { "extensions": [".js", ".jsx", ".ts", ".tsx"] }
      ]
   }
}
